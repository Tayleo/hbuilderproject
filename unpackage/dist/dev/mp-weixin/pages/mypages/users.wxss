page {
  background-color: #fff;
}
.header {
  background-color: #12ae85;
  width: 100%;
  height: 30vw;
  padding: 0 4%;
  display: flex;
  align-items: center;
}
.header.status {
  padding-top: 25px;
}
.header .userinfo {
  width: 90%;
  display: flex;
}
.header .userinfo .face {
  flex-shrink: 0;
  width: 17vw;
  height: 17vw;
}
.header .userinfo .face image {
  width: 100%;
  height: 100%;
  border-radius: 100%;
}
.header .userinfo .info {
  display: flex;
  flex-flow: wrap;
  padding-left: 30rpx;
}
.header .userinfo .info .username {
  width: 100%;
  color: #fff;
  font-size: 40rpx;
  margin-left: 10rpx;
}
.header .userinfo .info .integral {
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  height: 40rpx;
  color: #fff;
  background-color: rgba(0, 0, 0, 0.1);
  border-radius: 20rpx;
  font-size: 24rpx;
}
.header .setting {
  flex-shrink: 0;
  width: 6vw;
  height: 6vw;
}
.header .setting image {
  width: 100%;
  height: 100%;
}
.hover {
  background-color: #eee;
}
.orders {
  background-color: #12ae85;
  width: 100%;
  height: 11vw;
  padding: 0 4%;
  margin-bottom: calc(11vw + 40rpx);
  display: flex;
  align-items: flex-start;
  border-radius: 0 0 100% 100%;
  margin-top: -1rpx;
}
.orders .box {
  width: 98%;
  padding: 0 1%;
  height: 22vw;
  background-color: #fefefe;
  border-radius: 24rpx;
  box-shadow: 0 0 20rpx rgba(0, 0, 0, 0.15);
  margin-bottom: 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.orders .box .label {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-flow: wrap;
  width: 100%;
  height: 16vw;
  color: #666666;
  font-size: 26rpx;
}
.orders .box .label .icon {
  position: relative;
  width: 10vw;
  height: 10vw;
  margin: 0 1vw;
}
.orders .box .label .icon .badge {
  position: absolute;
  width: 4vw;
  height: 4vw;
  background-color: #ec6d2c;
  top: -1vw;
  right: -1vw;
  border-radius: 100%;
  font-size: 20rpx;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 10;
}
.orders .box .label .icon image {
  width: 7vw;
  height: 7vw;
  z-index: 9;
}
.orders .box .label .label-icon {
  font-size: 48rpx;
  color: #12ae85;
}
.list {
  width: 100%;
  border-bottom: solid 26rpx #f1f1f1;
}
.list .li {
  width: 92%;
  height: 100rpx;
  padding: 0 4%;
  border-bottom: solid 1rpx #e7e7e7;
  display: flex;
  align-items: center;
}
.list .li.noborder {
  border-bottom: 0;
}
.list .li .icon {
  flex-shrink: 0;
  width: 50rpx;
  height: 50rpx;
}
.list .li .icon image {
  width: 50rpx;
  height: 50rpx;
}
.list .li .text {
  padding-left: 30rpx;
  width: 100%;
  font-size: 28rpx;
  color: #000000;
}
.list .li .to {
  flex-shrink: 0;
  width: 40rpx;
  height: 40rpx;
}
.list .li .icon-next {
  font-size: 34rpx;
  color: #999999;
}

