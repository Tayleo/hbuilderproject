{
	"easycom": {
			"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
            "path" : "pages/mainpage/mainpage",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "首页",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/match/match",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "自动匹配",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/mypages/mypages",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "个人中心",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/info/info",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "详细信息",
                "enablePullDownRefresh": false
            }
            
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
"tabBar": {
	"color": "#7A7E83",
	"selectedColor": "#7A7E83",
	"borderStyle": "black",
	"backgroundColor": "#ffffff",
	"list": [{
		"pagePath": "pages/mainpage/mainpage",
		"iconPath": "static/icon/main.png",
		"selectedIconPath": "static/icon/main_se.png",
		"text": "首页"
	}, {
		"pagePath": "pages/match/match",
		"iconPath": "static/icon/match.png",
		"selectedIconPath": "static/icon/match_se.png",
		"text": "匹配"
	},{
		"pagePath": "pages/mypages/mypages",
		"iconPath": "static/icon/mine.png",
		"selectedIconPath": "static/icon/mine_se.png",
		"text": "我的"
	}]
}
	
}
